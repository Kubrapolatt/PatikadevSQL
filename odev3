--1.sorunun cevabi
SELECT*FROM country
WHERE country LIKE 'A%a';

--2.sorunun cevabi
SELECT country FROM country
WHERE country LIKE '_____n';

--3.sorunun cevabi
SELECT title FROM film
WHERE title ILIKE 'T%T%T%T%';

--4.sorunun cevabi
SELECT*FROM film
WHERE title LIKE 'C%' AND length>90 AND rental_rate=2.99;



